{
  "name": "monymony",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "restart": "npx expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "push:version-up": "git add . && git commit -m \"automated version up\"",
    "version-up": "node ./version-manager.js --all",
    "version-up:android": "node ./version-manager.js --android",
    "version-up:ios": "node ./version-manager.js --ios",
    "version-up:runtime": "node ./version-manager.js --runtime",
    "version-up:native": "node ./version-manager.js --all && node ./version-manager.js --runtime",
    "version-up:native:android": "node ./version-manager.js --android && node ./version-manager.js --runtime",
    "version-up:native:ios": "node ./version-manager.js --ios && node ./version-manager.js --runtime",
    "build-number-up:android": "node ./version-manager.js --android-build",
    "build-number-up:ios": "node ./version-manager.js --ios-build",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:all:dev": "eas build --platform all --profile development",
    "build:all:preview": "eas build --platform all --profile preview",
    "build:all:production": "eas build --platform all --profile production",
    "update:preview": "eas update --channel preview",
    "update:production": "eas update --channel production",
    "eas:submit": "eas submit --platform all --profile production",
    "eas:submit:android": "eas submit --platform android --profile production",
    "eas:submit:ios": "eas submit --platform ios --profile production",
    "publish-android": "eas build --platform android --auto-submit",
    "publish-ios": "eas build --platform ios --auto-submit",
    "review:beta:android": "cd android && fastlane beta_test",
    "review:production:android": "cd android && fastlane submit_review",
    "review:production:ios": "cd ios && fastlane submit_review",
    "beta:release:android": "npm run publish-android && npm run review:beta:android",
    "production:release:android": "npm run version-up:android && npm run publish-android && npm run review:production:android",
    "production:release:ios": "npm run version-up:ios && npm run publish-ios && npm run review:production:ios"
  },
  "codegenConfig": {
    "name": "NativeLocalStorageSpec",
    "type": "modules",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "com.nativelocalstorage"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~54.0.19",
    "expo-asset": "~12.0.10",
    "expo-audio": "~1.0.15",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "^0.32.12",
    "expo-router": "~6.0.13",
    "expo-sensors": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-version-check": "^3.5.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.9.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
